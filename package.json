{
  "name": "guess-the-color",
  "version": "1.0.0",
  "repository": "https://github.com/ZerdoX-x/guess-the-color",
  "description": "Try to guess color in different color spaces",
  "author": "ZerdoX-x",
  "scripts": {
    "dev": "run-p routify rollup",
    "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
