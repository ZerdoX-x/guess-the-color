    "ppid-changed": "^1.0.1",
    "pug": "^2.0.4",
    "rollup": "^2.6.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "spassr": "^1.0.2",
    "svelte-preprocess": "^3.7.4",
    "typescript": "^3.8.3"
  },
